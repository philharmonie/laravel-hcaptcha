{
    "name": "philharmonie/laravel-hcaptcha",
    "description": "Easy way to add hcaptcha to your laravel app",
    "keywords": [
        "philharmonie",
        "laravel-hcaptcha",
        "laravel",
        "hcaptcha",
        "captcha"
    ],
    "homepage": "https://github.com/philharmonie/laravel-hcaptcha",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Phil Harmonie",
            "email": "phil@harmonie.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.15",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Philharmonie\\LaravelHcaptcha\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Philharmonie\\LaravelHcaptcha\\LaravelHcaptchaServiceProvider"
            ],
            "aliases": {
                "LaravelHcaptcha": "Philharmonie\\LaravelHcaptcha\\LaravelHcaptchaFacade"
            }
        }
    }
}
